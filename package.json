{
  "name": "node-input-validator",
  "version": "3.1.0",
  "description": "validation library for nodejs, inspired by laravel.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive",
    "lint": "eslint --debug src/",
    "cover": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "coveralls": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --recursive -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "engines": {
    "node": ">=7.6"
  },
  "keywords": [
    "validation",
    "validator",
    "data validation",
    "input validation",
    "input validator",
    "form validation",
    "form validator",
    "node validator",
    "laravel",
    "validator for js",
    "koa",
    "express",
    "file validation",
    "image",
    "mimes"
  ],
  "repository": "uberforcede/input-validator",
  "author": {
    "name": "Koble dev team",
    "email": "koble@wefoxgroup.com"
  },
  "license": "ISC",
  "dependencies": {
    "file-type": "^10.7.0",
    "husky": "^4.2.3",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.21",
    "moment": "^2.23.0",
    "read-chunk": "^3.0.0",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/moment": "^2.13.0",
    "@types/validator": "^10.9.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "istanbul": "0.4.5",
    "mocha": "^5",
    "mocha-lcov-reporter": "^1.3.0",
    "ncp": "^2.0.0",
    "should": "^13.2.3"
  }
}
